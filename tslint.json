{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "semicolon": true,
      "quotemark": [true, "single", "jsx-double"],
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
      "interface-name": false,
      "no-console": false,
      "ordered-imports": false
    },
    "rulesDirectory": []
}
